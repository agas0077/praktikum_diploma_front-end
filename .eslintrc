{
    "parserOptions": {
        "ecmaVersion": 2017
    },

    "env": {
        "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "default-case":"off",
      "no-alert":"off",
      "no-console":"off",
      "no-underscore-dangle":"off",
      "class-methods-use-this":"off",
      "no-undef":"off",
      "no-plusplus":"off",
      "no-param-reassign":"off",
      "consistent-return":"off",
      "no-restricted-globals":"off",
    }
}
